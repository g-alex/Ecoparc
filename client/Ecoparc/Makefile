	@sed -e "s,@ICON@,icon.icns,g" -e "s,@EXECUTABLE@,Ecoparc,g" -e "s,@TYPEINFO@,????,g" /usr/local/Cellar/qt/4.7.4/mkspecs/macx-g++/Info.plist.app >Ecoparc.app/Contents/Info.plist
